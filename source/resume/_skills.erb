<div class="grid-wrap listings skills">
  <div class="section">
    <h2 id="skills">Skills include</h2>
  </div>
  <div class="content">
    <% [7..infinity, 4..6].each do |yrs| %>
      <div class="listing">
        <p>
          <strong>
            <%= yrs.begin %><%= yrs.end == infinity ? '+' : "-#{yrs.end}" %> years:
          </strong>
          <%= skills(all_jobs, yrs).join(', ') %>
        </p>
      </div>
    <% end %>
  </div>
</div>
